# Copy this file to secrets.toml and fill in your actual values
# DO NOT commit secrets.toml to version control

[snowflake]
user = "YOUR_USERNAME"
account = "YOUR_ACCOUNT_IDENTIFIER"  # e.g., "xy12345.us-east-1"
warehouse = "YOUR_WAREHOUSE"
database = "YOUR_DATABASE"
schema = "YOUR_SCHEMA"
role = "YOUR_ROLE"  # Optional
private_key_path = "/path/to/your/private_key.p8"  # Path to your private key file
private_key_passphrase = ""  # Optional, only if your key is encrypted

